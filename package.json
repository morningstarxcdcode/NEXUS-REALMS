  "author": "Nexus Realms Team",
  "license": "MIT",
  "dependencies": {
    "three": "^0.158.0",
    "cannon-es": "^0.20.0",
    "howler": "^2.2.3",
    "gsap": "^3.12.2",
    "dat.gui": "^0.7.9"
  },
  "devDependencies": {
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "babel-loader": "^9.1.3",
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "html-webpack-plugin": "^5.5.3",
    "css-loader": "^6.8.1",
    "style-loader": "^4.0.0",
    "file-loader": "^6.2.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
