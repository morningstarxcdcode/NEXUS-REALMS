{
  "name": "nexus-realms",
  "version": "1.0.0",
  "description": "An epic adventure game with multiple endings and cutting-edge features",
  "main": "src/main.js",
  "type": "module",
  "scripts": {
    "start": "node src/main.js",
    "dev": "node --watch src/main.js",
    "build": "webpack --mode production",
    "build:web": "webpack --config webpack.web.js",
    "build:mobile": "cordova build",
    "test": "jest",
    "lint": "eslint src/",
    "optimize": "node scripts/optimize-assets.js"
  },
  "keywords": [
    "game",
    "adventure",
    "rpg",
    "multiple-endings",
    "hackathon",
    "webgl",
    "cross-platform"
